# The primary network interface
auto eth0
auto eth0:0
auto eth0:1

iface eth0 inet static
address 192.168.0.10
netmask 255.255.255.0
gateway 192.168.0.1

iface eth0:0 inet static
address 192.168.0.105
netmask 255.255.255.0
gateway 192.168.0.1

iface eth0:1 inet static
address 192.168.0.120
netmask 255.255.255.0
gateway 192.168.0.1
